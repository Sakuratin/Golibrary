page {
  width: 100%;
  height: 100%;
}

#container {
  background: linear-gradient(to left, #2B85D7,#7C51DC);
}

.placeholder-class { /* WebKit browsers */
  color: #ffffffc2;
  font-size: 30rpx;
}


.search {
  text-align: center;
}

.search .welcom {
  width: 85%;
  margin: 25rpx auto;
  text-align: left;
  color: white;
  padding-left: 20rpx;
  font-size: 40rpx;
}

.welcom>div:nth-of-type(1) {
  font-size: 13rpx;
  font-weight: normal;
  color: #cbcbcb;
}

.welcom>div:nth-of-type(2) {
  font-size: 20rpx;
  font-weight: bold;
}

.search>input {
  width: 85%;
  padding: 15rpx 0 15rpx 29rpx;
  border-radius: 9rpx;
  border: none;
  margin: auto;
  background-color: #ffffff4d;
  color: white;
  margin-top: 40rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: 20rpx;
  background-size: 40rpx;
}

#book-list {
  background-color: white;
  padding-top: 25rpx;
  border-top-left-radius: 25rpx;
  border-top-right-radius: 25rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-top: 55rpx;
}

.book-item {
  display: flex;
  padding-left: 5rpx;
  margin-top: 25rpx;
}

.book-item image {
  width: 240rpx;
  height: 340rpx;
  /* min-width: 650rpx;
  min-height: 400rpx; */
  border-radius: 25rpx;
  border: 1px solid #cfcfcf;
}

.book-info {
  padding-left: 20rpx;
  padding-top: 2rpx;
  line-height: 50rpx;
}

.book-info .name {
  font-size: 35rpx;
  font-weight: bold;
  font-family: Helvetica, 'Hiragino Sans GB', 'Microsoft Yahei', '微软雅黑', Arial, sans-serif;
}

.info-text {
  color: #888888;
  font-size: 30rpx;
  margin-top: 5rpx;
}

.book-info .publisher>span:nth-of-type(2) {
  margin-left: 30rpx;
}

